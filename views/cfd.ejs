
<table>
<% cfds.forEach ((_cfd, i)=>{%>
			<tr>
			<td width=20%><%=_cfd.name%></td>
			<td width=20%><%=_cfd.value%></td>
			<td width=20%><%=_cfd.consensus%></td>
			<td width=20%><%=(_cfd.delta/_cfd.value*100).toFixed(2) %>%
			<td width=20%><img src="http://www.nasdaq.com/charts/<%=_cfd.name%>_smallrm.jpeg"/></td>
			</tr>
			<%			});%>
			</table>